
  CREATE TABLE "NIRMAL_GURJAR"."BULK_REFUND_BOOKING_DTLS" 
   (	"ID" RAW(16) DEFAULT SYS_GUID(), 
	"BULK_ID" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"ROW_NUMBER" NUMBER DEFAULT 0, 
	"REFUND_STATUS" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"REMARK" VARCHAR2(500 BYTE) NOT NULL ENABLE, 
	"CREATED_BY" VARCHAR2(255 BYTE), 
	"UPDATED_BY" VARCHAR2(255 BYTE), 
	"CREATED_DATE_NUM" NUMBER DEFAULT 0, 
	"UPDATED_DATE" NUMBER, 
	"CREATED_DATE" DATE DEFAULT SYSDATE
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
  STORAGE(
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "DEV_TEST_USERS" 
  PARTITION BY RANGE ("CREATED_DATE") INTERVAL (NUMTOYMINTERVAL(1, 'MONTH')) 
 (PARTITION "P_START"  VALUES LESS THAN (TO_DATE(' 2022-01-01 00:00:00', 'SYYYY-MM-DD HH24:MI:SS', 'NLS_CALENDAR=GREGORIAN')) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 


  CREATE TABLE "NIRMAL_GURJAR"."REFUND_BOOKING" 
   (	"ID" RAW(16) DEFAULT SYS_GUID(), 
	"MERCHANT_ID" VARCHAR2(20 BYTE), 
	"SBI_ORDER_REF_NUMBER" VARCHAR2(50 BYTE), 
	"ARRN_NUM" VARCHAR2(255 BYTE) NOT NULL ENABLE, 
	"ATRN_NUM" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"REFUND_AMOUNT" NUMBER(20,2) DEFAULT 0 NOT NULL ENABLE, 
	"REFUND_TYPE" VARCHAR2(50 BYTE), 
	"REFUND_STATUS" VARCHAR2(50 BYTE), 
	"REMARK" VARCHAR2(500 BYTE), 
	"CREATED_BY" VARCHAR2(255 BYTE) NOT NULL ENABLE, 
	"UPDATED_BY" VARCHAR2(255 BYTE), 
	"CREATED_DATE" NUMBER NOT NULL ENABLE, 
	"UPDATED_DATE" NUMBER, 
	 SUPPLEMENTAL LOG DATA (ALL) COLUMNS
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "DEV_TEST_USERS" ;
  CREATE UNIQUE INDEX "NIRMAL_GURJAR"."REFUND_BOOKING_ID_IDX" ON "NIRMAL_GURJAR"."REFUND_BOOKING" ("ID") 
  PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  TABLESPACE "DEV_TEST_USERS" ;
  CREATE UNIQUE INDEX "NIRMAL_GURJAR"."REFUND_BOOKING_ARRN_IDX" ON "NIRMAL_GURJAR"."REFUND_BOOKING" ("ARRN_NUM") 
  PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  TABLESPACE "DEV_TEST_USERS" ;
ALTER TABLE "NIRMAL_GURJAR"."REFUND_BOOKING" ADD PRIMARY KEY ("ID")
  USING INDEX "NIRMAL_GURJAR"."REFUND_BOOKING_ID_IDX"  ENABLE;
ALTER TABLE "NIRMAL_GURJAR"."REFUND_BOOKING" ADD UNIQUE ("ARRN_NUM")
  USING INDEX "NIRMAL_GURJAR"."REFUND_BOOKING_ARRN_IDX"  ENABLE;


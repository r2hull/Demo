
  CREATE OR REPLACE FORCE EDITIONABLE VIEW "RAHUL_Y"."VIEW_REFUND_DASHBOARD_SUMMARY" ("MERCHANT_ID", "REFUND_COUNT", "TOTAL_AVAILABLE_AMT", "TOTAL_REFUND_AMT", "CREATED_DATE") AS 
  select "MERCHANT_ID","REFUND_COUNT","TOTAL_AVAILABLE_AMT","TOTAL_REFUND_AMT","CREATED_DATE" from PAYAGGREPORT.view_refund_dashboard_summary;


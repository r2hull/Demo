@Data
public class KeyGenerationRequest {
    @NotEmpty(message = MANDATORY_ERROR_MESSAGE)
    private String mId;
    @NotNull(message = MANDATORY_ERROR_MESSAGE)
    private Integer keyLength;//This is for encryption
    @NotNull(message = MANDATORY_ERROR_MESSAGE)
    private Integer expiry; //in hrs
    @NotNull(message = MANDATORY_ERROR_MESSAGE)
    @Min(value = 1, message = MIN_VALUE_ERROR_MESSAGE + MIN_VAL_NEW_KEY_EXPIRY_TIME)
    private Integer newKeyExpiryTime;
}

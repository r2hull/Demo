@Data
@Builder
@AllArgsConstructor
@NoArgsConstructor
public class MerchantEntityGroupRequest {

    @Schema(name = "entityId", example = "ENTITY001")
    private String entityId;
    @Schema(name = "mIds",description = "list of mids to associate with the entity", example = "[\"MID6\"]")
    private List<String> mIds;

}

public List<MerchantUser> getFirstUsersByUserNameForMid(String mid) {
    List<MerchantUser> users = merchantUserRepository.findAllUsersByMid(mid);

    return users.stream()
            .collect(Collectors.groupingBy(MerchantUser::getUserName))
            .values()
            .stream()
            .map(group -> group.stream()
                    .sorted(Comparator.comparing(MerchantUser::getCreatedAt))
                    .findFirst().orElse(null))
            .filter(Objects::nonNull)
            .collect(Collectors.toList());
}
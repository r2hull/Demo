@Data
@Builder
@NoArgsConstructor
@AllArgsConstructor
public class UserValidationRequest {
    @NotBlank(message = " is mandatory")
    @Schema(name = "requestType", example = "LOGIN")
    private String requestType;
    @NotBlank(message = " is mandatory")
    @Schema(name = "userName", example = "user5")
    private String userName;
}

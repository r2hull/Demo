public class MerchantUserStatusRequest {
    @Schema(description = "userName", example = "user6+")
    private String userName;
    @Schema(name = "userStatus",description = "UserStatus Valid Status are ACTIVE, INACTIVE, BLOCKED, EXPIRED",example = "ACTIVE")
    private UserStatus userStatus;
    @Schema(description = "remark", example = "User status updated successfully")
    private String remark;
}

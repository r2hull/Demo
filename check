    private static BulkRefundBooking buildBulkRefundBooking(BulkRefundRequest bulkRefundRequest, String filePath) {

        return BulkRefundBooking.builder()
                .bulkRefundStatus(BulkRefundStatus.IN_QUEUE.name())
                .fileName(bulkRefundRequest.getFileName())
                .filePath(filePath)
                .merchantId(bulkRefundRequest.getMId())
                .createdBy(TransactionConstant.BULK_REFUND_SERVICE)
                .createdDate(DateTimeUtils.getCurrentTimeInMills())
                .build();
    }

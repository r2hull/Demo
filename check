  @Test
    void testGetNotices(){
        String mId = "1000003";
        List<HelpSupportDto> helpSupportDtos = List.of(HelpSupportDto.builder().mId("1000003").type("IMPORTANT_NOTICE").build());
        HelpSupport helpSupport = HelpSupport.builder().mId(mId).type(IMPORTANT_NOTICE).build();
        List<HelpSupport> helpSupportList = List.of(helpSupport);
        doNothing().when(mIdValidator).validateMid(mId);
        doNothing().when(mIdValidator).validateActiveMId(mId);
        when(helpSupportRepository.findByMidTypeStatus(mId, IMPORTANT_NOTICE, STATUS_ACTIVE)).thenReturn(helpSupportList);
        when(helpSupportMapper.mapEntityListToDtoList(helpSupportList)).thenReturn(helpSupportDtos);
        when(helpSupportDao.getNotices(mId)).thenReturn(helpSupportDtos);
        MerchantResponse<HelpSupportResponse> serviceNotices = helpSupportService.getNotices(mId);
        assertEquals(serviceNotices.getStatus(), 1);
    }

   @PostMapping("/mid/mapping/{userName}")
    @Operation(summary = "Assigned MIDs to Users", description = "Get Assigned and UnAssigned Merchant Info for given User")
    public MerchantResponse<String> assignedMIdsToUser(@Parameter(description = "Username of the merchant user", required = true) @PathVariable String userName, @Parameter(description = "List of MIDs to be assigned", required = true) @NotEmpty(message = ErrorConstants.MANDATORY_FIELD) @RequestBody List<String> assignedMIds) {
        log.info("Received request to assign MIDs to User");
        return merchantUserService.assignedMIdsToUser(userName, assignedMIds);
    }

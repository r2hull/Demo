@Query("SELECT m FROM MerchantUser m " +
       "JOIN MerchantEntityUser meu ON m.id = meu.userId " +
       "JOIN MerchantEntityGroup meg ON meu.mId = meg.mId " +
       "WHERE meu.mId = :mid AND (meu.entityId IS NULL OR meu.entityId IN (SELECT meg2.entityId FROM MerchantEntityGroup meg2 WHERE meg2.mId = :mid))")
Page<MerchantUser> findBymId(@Param("mid") String mId, Pageable pageable);
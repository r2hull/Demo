
  CREATE TABLE "RAHUL_Y"."RECON_FILE_SUMMARY" 
   (	"RFS_ID" RAW(16), 
	"RFC_ID" RAW(16), 
	"BANK_CODE" VARCHAR2(50 BYTE), 
	"SFTP_PATH" VARCHAR2(250 BYTE), 
	"S3_PATH" VARCHAR2(250 BYTE), 
	"FILE_NAME" VARCHAR2(100 BYTE), 
	"FILE_RECEIVED_TIME" NUMBER, 
	"FILE_UPLOAD_STATUS" VARCHAR2(10 BYTE), 
	"FILE_UPLOAD_TIME" NUMBER, 
	"PARSING_STATUS" VARCHAR2(10 BYTE), 
	"TOTAL_AMOUNT" NUMBER(20,2), 
	"TOTAL_RECORDS" NUMBER, 
	"MATCHED_RECORDS" NUMBER, 
	"UNMATCHED_RECORDS" NUMBER, 
	"DUPLICATE_RECORDS" NUMBER, 
	"PASSED_RECORDS" NUMBER, 
	"FAILED_RECORDS" NUMBER, 
	"RECON_STATUS" VARCHAR2(10 BYTE), 
	"RECON_TIME" NUMBER, 
	"SETTLEMENT_STATUS" VARCHAR2(10 BYTE), 
	"SETTLEMENT_TIME" NUMBER, 
	"REMARK" VARCHAR2(500 BYTE), 
	"FILE_CHECKSUM" VARCHAR2(512 BYTE), 
	"PAYOUT_STATUS" VARCHAR2(10 BYTE)
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "DEV_TEST_USERS" ;



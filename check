SELECT * FROM (SELECT
M.*,ROW_NUMBER () OVER (PARTITION BY  m.id ORDER BY m.id ) R
FROM
merchant_user        m,
merchant_entity_user  meu,
merchant_entity_group meg
WHERE
m.id = meu.user_id AND ( ( meu.mid = '1000003' AND meu.entity_id IS NULL ) OR ( meg.entity_id = meu.entity_id AND meg.mid = '1000003'))) WHERE R=1
OFFSET 0 ROWS
FETCH NEXT 5 ROWS ONLY;

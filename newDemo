@Builder
public class MerchantBankAccountsDetailDto {

    @JsonProperty("mId")
    private String mId;
    private BankAccountDto primaryAccount;
    private List<BankAccountDto> secondaryAccount;

}

@JsonInclude(JsonInclude.Include.NON_NULL)
public class BankAccountDto {

    private String bankId;
    private String accountId;
    private String accountNumber;
    private String accountNickName;
    private String primaryAccount; //Y, N
    @JsonIgnore
    private String mId;

}
@Builder
public class MultiAccountDto {

    private String accountIdentifier;
    private BigDecimal amount;
}
  

List<MultiAccountDto> multiAccountDetails = orderValidator.getMultiAccountDtos(orderDto);
        MerchantBankAccountsDetailDto bankDetails = adminDao.getBankDetails(List.of("1000003"));
        if(multiAccountDetails.isEmpty()){
            String sbiOrderRefNumber = orderDto.getSbiOrderRefNumber(); //orderId 5D4F137CA9CD4BDBACBE
            String accountNickName = bankDetails.getPrimaryAccount().getAccountNickName();
            String accountNumber = bankDetails.getPrimaryAccount().getAccountNumber();
            MerchantOrderAccountInfo.builder().orderId(sbiOrderRefNumber)
                    .accountHeader(accountNickName).amount(orderDto.getOrderAmount()).accountNumber(accountNumber)
                    .accountType(AccountType.P).accountClassification(orderDto.getAccountClassification()).build();
        }else{

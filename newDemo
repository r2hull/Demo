
@JsonIgnoreProperties({"createdBy","updatedBy","createdDate","updatedDate"})
public class OrderDto extends BaseDto {
    @JsonProperty("mId")
    private String mId;
    private String customerId;
    private String currencyCode;
    private BigDecimal orderAmount;
    private String orderRefNumber;
    private String sbiOrderRefNumber;//orderId
    private OrderStatus status;
    private JsonNode otherDetails;
    private Long expiry;
    private JsonNode multiAccounts;
    private String paymentMode;
    private String orderHash;
    private String returnUrl;
    private String transactionUrl;
    private Integer orderRetryCount;
    private JsonNode thirdPartyDetails;
}
@Builder
public class MerchantBankAccountsDetailDto {

    @JsonProperty("mId")
    private String mId;
    private BankAccountDto primaryAccount;
    private List<BankAccountDto> secondaryAccount;

}


@JsonInclude(JsonInclude.Include.NON_NULL)
public class BankAccountDto {

    private String bankId;
    private String accountId;
    private String accountNumber;
    private String accountNickName;
    private String primaryAccount;
    @JsonIgnore
    private String mId;

}


public class MultiAccountDto {

    private String accountIdentifier;
    private BigDecimal amount;
}


if multiAccounts is null  
toh MerchantBankAccountsDetailDto isme se primary account ka data MerchantOrderAccountInfo me save krna hai

if null nhi hai toh  MultiAccountDto me se accountIdentifier nikal ke MerchantBankAccountsDetailDto isme se match krna hai jo match hua toh set krna hai


isme save krna hai 
@Table(name = "MERCHANT_ORDER_ACCOUNT_INFO")
public class MerchantOrderAccountInfo  extends AuditEntity {
    @Id
    @GeneratedValue(strategy = GenerationType.UUID)
    @Column(nullable = false, updatable = false)
    private UUID id;
    private String orderId; //sbiOrderRefNumber
    private String accountHeader; //accountNickName
    private BigDecimal amount;
    private String accountNumber;

    @Enumerated(EnumType.STRING)
    private AccountType accountType; //   P,S;

    @Enumerated(EnumType.STRING)
    private AccountClassification accountClassification; // TREASURY,NON_TREASURY;

}

 public TransactionResponse<String> createMerchantOrder(EncryptedRequest encryptedRequest) {

        OrderDto orderDto = buildRequestByEncryptRequest(encryptedRequest.getEncryptedRequest(), mek, OrderDto.class);
        logger.debug("build OrderDto{} ", orderDto);
        // Step 3 : Validate and create order.
        orderDto = createMerchantOrder(orderDto);
        String sbiOrderRefNumber = orderDto.getSbiOrderRefNumber(); //orderId
        List<MultiAccountDto> multiAccountDetails = orderValidator.getMultiAccountDtos(orderDto);
        MerchantBankAccountsDetailDto bankDetails = adminDao.getBankDetails(List.of("1000003"));


//MerchantOrderAccountInfo save
        //Step 4 : Create order response
        logger.info("Merchant Order creation completed");
        return TransactionResponse.<String>builder().data(List.of(EncryptionDecryptionUtil.encryptValue(mek, TransactionUtil.toJson(orderDto)))).status(TransactionConstant.RESPONSE_SUCCESS).build();
    }

@Test
void testGetNewMIdMappingListWithReflection() throws Exception {
    List<String> mIds = Arrays.asList("mid1", "mid2", "mid3");
    List<MerchantInfoDto> assignedMIds = Arrays.asList(
            MerchantInfoDto.builder().mId("mid1").build(),
            MerchantInfoDto.builder().mId("mid2").build()
    );

    Method method = MerchantUserService.class.getDeclaredMethod("getNewMIdMappingList", List.class, List.class);
    method.setAccessible(true); // Make the private method accessible

    @SuppressWarnings("unchecked")
    List<String> result = (List<String>) method.invoke(null, mIds, assignedMIds);

    assertEquals(1, result.size());
    assertTrue(result.contains("mid3"));
}
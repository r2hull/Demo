   public List<MerchantFeesReport> getMerchantFeesInvoice(String mId, List<String> reportDates) {
        SqlParameterSource parameters = new MapSqlParameterSource(Map.of("mId", mId, "reportDate", reportDates));
        return namedParameterJdbcTemplate.query(InvoiceQueries.JDBC_TXN_INVOICE, parameters, new BeanPropertyRowMapper<>(MerchantFeesReport.class));
    }

@Repository
public interface MerchantPricingCacheRepository extends CrudRepository<MerchantPricingEntity, String> {

    @Query("SELECT t FROM /Admin_Pricing_View t WHERE t.mId = :mId " +
            "AND t.payModeCode = :payModeCode " +
            "AND t.gtwMapsId = :gtwMapsId " +
            "AND t.payProcType = :payProcType " +
            "AND t.slabFrom <= :txnAmount " +
            "AND t.slabTo >= :txnAmount")
    List<MerchantPricingEntity> getPricing(@Param("mId") String mId, 
                                           @Param("payModeCode") String payModeCode, 
                                           @Param("gtwMapsId") String gtwMapsId, 
                                           @Param("payProcType") String payProcType, 
                                           @Param("txnAmount") BigDecimal txnAmount);
}
  List<Map<String, Object>> merchantVolumeDetailsData = (List<Map<String, Object>>) aggMerchantVolumeVelocityRepository.getMerchantVolumeDetails(mid);
        Optional.ofNullable(merchantVolumeDetailsData).filter(x -> !x.isEmpty()).orElseThrow(() -> new AdminException(ErrorConstants.VOLUME_ERROR_CODE, ErrorConstants.VOLUME_ERROR_MESSAGE));

        merchantVolumeDetailsData.stream().map(stringObjectMap -> {
            stringObjectMap
            
            return null;
        });
        
        AggMerchantVolumeVelocityEntity aggMerchantVolumeVelocityEntity=new AggMerchantVolumeVelocityEntity();
        BeanUtils.copyProperties(merchantVolumeDetailsData, aggMerchantVolumeVelocityEntity);
        // Cache the fetched data for future use
        aggMerchantVolumeVelocityCacheRepository.saveAll(aggMerchantVolumeVelocityEntity);
        return merchantVolumeDetailsData;


public class AggMerchantVolumeVelocityEntity {
    @Id
    private String merchant_Vvl_Id;
    private String paymodeCode;
    private char volvelcheckAllowed;
    private BigDecimal minTxnLimit;
    private BigDecimal max_txn_Limit;
    private BigDecimal dailyTxnAmtLimit;
    private BigDecimal weeklyTxnAmtLimit;
    private BigDecimal monthlyTxnamtLimit;
    private BigDecimal quarterlyTxnAamtLimit;
    private BigDecimal halfyearlyTxnNoLimit;
    private BigDecimal annualTxnAmtLimit;

    public AggMerchantVolumeVelocityEntity() {
    }
}


  @Query(value = "select MERCHANT_ID as merchantId,PAY_MODE_CODE as payModeCode,VOLVEL_CHECK_ALLOWED as volumeCheckAllowed,MIN_TXN_LIMIT as minTxnLimit, MAX_TXN_LIMIT as maxTxnLimit, DAILY_TXN_AMT_LIMIT as dailyTxnAmountLimit, WEEKLY_TXN_AMT_LIMIT as WeeklyTxnAmountLimit, MONTHLY_TXN_AMT_LIMIT as monthlyTxnAmountLimit, " +
           " QUARTERLY_TXN_AMT_LIMIT as quaterlyTxnAmountLimit, HALFYEARLY_TXN_NO_LIMIT as halfYearlyTxnAmountLimit, ANNUAL_TXN_AMT_LIMIT as annualTxnAmountLimit " +
           " from MERCHANT_VVL_VIEW where merchant_id=:mid",nativeQuery = true)
   List<Map<String, Object>> getMerchantVolumeDetails(@Param(value = "mid") String mid);

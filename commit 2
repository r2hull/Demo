BEGIN
  FOR t IN (SELECT table_name FROM all_tables WHERE owner = 'PAYAGGTRANSACTIO') LOOP
    BEGIN
      EXECUTE IMMEDIATE 'CREATE TABLE ' || t.table_name || ' AS SELECT * FROM PAYAGGTRANSACTIO.' || t.table_name;
    EXCEPTION
      WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE('Failed to copy table: ' || t.table_name || ' - ' || SQLERRM);
    END;
  END LOOP;
END;
/
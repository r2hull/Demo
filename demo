ALTER TABLE LIST_OF_MASTER ADD (
    CODE VARCHAR2(100) NOT NULL,
    DESCRIPTION VARCHAR2(500)
);

ALTER TABLE LIST_OF_MASTER ADD CONSTRAINT UK_MASTER_CODE UNIQUE (CODE);


ALTER TABLE LIST_OF_VALUE ADD (
    CODE VARCHAR2(100) NOT NULL,
    DESCRIPTION VARCHAR2(500),
    PARENT_CODE VARCHAR2(100)
);

ALTER TABLE LIST_OF_VALUE ADD CONSTRAINT UK_VALUE_CODE UNIQUE (CODE);


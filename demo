    public List<TransactionTrackingResponse> getResponse(ReportResponse reportResponse,String payMode,String channelBank){
        List<TransactionTrackingResponse> data = reportResponse.getData();
        if(payMode!=null){
            data=data.stream().filter(transactionTrackingResponse -> transactionTrackingResponse.getPayMode().equalsIgnoreCase(payMode)).collect(Collectors.toList());
        }
        if(channelBank!=null){
            data=data.stream().filter(transactionTrackingResponse -> transactionTrackingResponse.getChannelBank().equalsIgnoreCase(channelBank)).collect(Collectors.toList());
        }
